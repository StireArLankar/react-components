{
  "plugins": ["prettier"],
  "extends": ["prettier", "react-app"],
  "rules": {
    "react/self-closing-comp": "warn",
    "prettier/prettier": [
      "warn",
      {},
      {
        "usePrettierrc": true
      }
    ],
    "curly": "warn",
    "no-lonely-if": "warn",
    "no-else-return": "warn",
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ]
      }
    ]
  }
}
